{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "availabilityZones": {
            "value": []
        },
        "containerName": {
            "value": "CONTAINER-NAME-PLACEHOLDER"
        },
        "location": {
            "value": "AZ_LOCATION_PLACEHOLDER"
        },
        "imageType": {
            "value": "Private"
        },
        "imageName": {
            "value": "DOCKER_USER/DOCKER_IMG:TAG"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "1"
        },
        "memory": {
            "value": "1.5"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "sku": {
            "value": "Standard"
        },
        "imageRegistryLoginServer": {
            "value": "index.docker.io"
        },
        "imageUsername": {
            "value": "DOCKER_USER"
        },
        "imagePassword": {
            "value": "DOCKER_PASSWORD"
        },
        "environmentVariable_cr_target_port": {
            "value": "CONTAINER_EXPOSED_PORT_NUMBER"
        },
        "environmentVariable_flask_key": {
            "value": "FLASK_KEY_PLACEHOLDER"
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "80",
                    "protocol": "TCP"
                }
            ]
        }
    }
}
